<script>
    $('#clipcontrol').hide();
    $('#pags').hide();
    $('#foreground').hide();
    $('#maincontentdiv').hide();
    // $('#maincontentdiv').fadeIn(1500);

    $('#maincontentdiv').animate({top: "600px"}, 0 );
    $('#maincontentdiv').show();
    $('#maincontentdiv').animate({top: "101px"}, 1000 );
</script>

<br />
<h1>Search Results</h1>
<br />
<hr />

<% if @users.empty? %>
  <div>No results.</div>
<% else %>
  <div class="tabular_container">
    <% @users.each do |user| %>
      <% if user != current_user and !current_user.contacts.include?(user)%>
        <div class="row">
	  <div class="column" id="contact<%= user.id %>">
	    <%= render({layout: 'users/search_result', locals: {user: user}}) do end %>
	  </div>
	</div>
      <% end %>
    <% end %>
  </div>
<% end %>
