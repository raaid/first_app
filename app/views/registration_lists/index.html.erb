<h1>Listing registration_lists</h1>

<table>
  <tr>
    <th>Event</th>
    <th></th>
    <th></th>
    <th></th>
  </tr>

<% @registration_lists.each do |registration_list| %>
  <tr>
    <td><%= registration_list.event %></td>
    <td><%= link_to 'Show', registration_list %></td>
    <td><%= link_to 'Edit', edit_registration_list_path(registration_list) %></td>
    <td><%= link_to 'Destroy', registration_list, method: :delete, data: { confirm: 'Are you sure?' } %></td>
  </tr>
<% end %>
</table>

