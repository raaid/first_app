BEGIN:VCALENDAR  
PRODID:-//Eventastic//EN  
VERSION:1.0  
BEGIN:VEVENT  
SUMMARY:<%= raw @event.name %>  
PRIORITY:0   
CLASS:PUBLIC  
DTSTART:<%= @event.startTime.utc.strftime("%Y%m%dT%H%M%SZ") %>  
DTEND:<%= @event.endTime.utc.strftime("%Y%m%dT%H%M%SZ") %>  
URL:http://www.eventastic.com/events/<%= @event.id %>
DESCRIPTION:<%= raw @event.description.gsub(/\n/,"").gsub(/\r/, "") %>\n<%= raw @event.details.gsub(/\n/,"").gsub(/\r/, "") %>
LOCATION:<%= raw @event.address %>  
END:VEVENT  
END:VCALENDAR  