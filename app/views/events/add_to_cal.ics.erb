BEGIN:VCALENDARVERSION:2.0PRODID:-//Eventastic//ENBEGIN:VEVENTUID:<%= Time.zone.now.strftime("%Y%m%dT%H%M%SZ") %>@eventastic.comDTSTAMP:<%= Time.zone.now.strftime("%Y%m%dT%H%M%SZ") %>DTSTART:<%= @event.startTime.strftime("%Y%m%dT%H%M00Z") %>DTEND:<%= @event.endTime.strftime("%Y%m%dT%H%M00Z") %>URL:http://www.eventastic.com/events/<%= @event.id %>SUMMARY:<%= raw @event.name %>LOCATION:<%= raw @event.address %>DESCRIPTION:<%= raw @event.description.gsub(/\n/,"").gsub(/\r/, "") %>\n<%= raw @event.details.gsub(/\n/,"").gsub(/\r/, "") %>END:VEVENTEND:VCALENDAR