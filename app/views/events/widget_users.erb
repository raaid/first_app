<div style="height:100%;width:100%;display:inline-block;">
	<%@events.each do |event|%>
		<div onclick="window.open('<%=event_path(event)%>', '_blank')" style="cursor:pointer;padding:8px;border-radius:8px;border: solid 1px #fff;margin-top:5px;background-color:rgba(50,47,50,0.8);width:98%;color:#fff;font-size:17pt;-webkit-box-shadow:rgba(0,0,0,0.6) 0 6px 15px; display: inline-block">
			<img src="<%=event.event_image.url(:feed)%>" style="height:35px;float:left"/>
			<div style="float:left;margin-left:10px"><%=event.name%></div>
			<div style="float:right"><%= event.startTime.strftime("%Y-%m-%d") %></div>
		</div>
	<%end%>
</div>