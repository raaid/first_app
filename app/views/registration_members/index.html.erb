<h1>Listing registration_members</h1>

<table>
  <tr>
    <th>Registration list</th>
    <th>Name</th>
    <th>Email</th>
    <th></th>
    <th></th>
    <th></th>
  </tr>

<% @registration_members.each do |registration_member| %>
  <tr>
    <td><%= registration_member.registration_list %></td>
    <td><%= registration_member.name %></td>
    <td><%= registration_member.email %></td>
    <td><%= link_to 'Show', registration_member %></td>
    <td><%= link_to 'Edit', edit_registration_member_path(registration_member) %></td>
    <td><%= link_to 'Destroy', registration_member, method: :delete, data: { confirm: 'Are you sure?' } %></td>
  </tr>
<% end %>
</table>

<br />

<%= link_to 'New Registration member', new_registration_member_path %>
